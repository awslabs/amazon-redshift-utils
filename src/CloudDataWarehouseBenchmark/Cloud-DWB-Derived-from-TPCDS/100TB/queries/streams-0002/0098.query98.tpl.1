
select /* query_templates/query98.tpl.0 !CF:IR-c4b99a30-f08a-11e9-b661-06872b3fecc8..stream-query.streams-0002.i0098.1.0:CF! */ i_item_id
      ,i_item_desc 
      ,i_category 
      ,i_class 
      ,i_current_price
      ,sum(ss_ext_sales_price) as itemrevenue 
      ,sum(ss_ext_sales_price)*100/sum(sum(ss_ext_sales_price)) over
          (partition by i_class) as revenueratio
from	
         public.store_sales
    	,public.item 
    	,public.date_dim
where 
	ss_item_sk = i_item_sk 
  	and i_category in ('Children', 'Electronics', 'Music')
  	and ss_sold_date_sk = d_date_sk
	and d_date between cast('2002-04-08' as date) 
				and (cast('2002-04-08' as date) + interval '30 days')
group by 
	i_item_id
        ,i_item_desc 
        ,i_category
        ,i_class
        ,i_current_price
order by 
	i_category
        ,i_class
        ,i_item_id
        ,i_item_desc
        ,revenueratio;
