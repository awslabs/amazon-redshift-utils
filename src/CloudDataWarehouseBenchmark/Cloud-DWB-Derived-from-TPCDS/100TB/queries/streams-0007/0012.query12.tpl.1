
select /* query_templates/query12.tpl.0 !CF:IR-c4b99a30-f08a-11e9-b661-06872b3fecc8..stream-query.streams-0007.i0012.1.0:CF! */  i_item_id
      ,i_item_desc 
      ,i_category 
      ,i_class 
      ,i_current_price
      ,sum(ws_ext_sales_price) as itemrevenue 
      ,sum(ws_ext_sales_price)*100/sum(sum(ws_ext_sales_price)) over
          (partition by i_class) as revenueratio
from	
	public.web_sales
    	,public.item 
    	,public.date_dim
where 
	ws_item_sk = i_item_sk 
  	and i_category in ('Music', 'Men', 'Shoes')
  	and ws_sold_date_sk = d_date_sk
	and d_date between cast('1999-01-03' as date) 
				and (cast('1999-01-03' as date) + '30 days'::interval)
group by 
	i_item_id
        ,i_item_desc 
        ,i_category
        ,i_class
        ,i_current_price
order by 
	i_category
        ,i_class
        ,i_item_id
        ,i_item_desc
        ,revenueratio
limit 100;
